SEQUENCE @startuml

participant FA
participant GA
participant PL


FA -> GA : POST "/garden/plant/${plantType}"
activate GA
GA -> FA : 200 - (plantId : "plantId")

GA -> PL : GET "/plant/season/${plantType}"
activate PL
PL -> GA : 200 - (season : "season")

FA -> GA : GET "/garden/seeds/${plantType}"
GA -> FA : 200 - (numberOfSeeds : "numberOfSeeds")

GA -> PL : GET "/plant/timetogrow/${plantType}"

PL -> GA : 200 - (timetogrow : "timetogrow")
deactivate PL

GA -> FA : POST "/farmer/grown" (plantId : "${plantId}")
FA -> GA : 200

deactivate GA

@enduml
